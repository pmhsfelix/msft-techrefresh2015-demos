{
  "webroot": "wwwroot",
  "version": "1.0.0-*",

  "dependencies": {
    "Microsoft.AspNet.Server.IIS": "1.0.0-beta4",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-beta4",
    "Microsoft.Framework.Logging.Console": "1.0.0-beta4",
    "Microsoft.AspNet.Diagnostics": "1.0.0-beta4",
    "Microsoft.AspNet.Hosting": "1.0.0-beta4",
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-beta4",
    "Kestrel": "1.0.0-beta4",
    "Microsoft.AspNet.Http": "1.0.0-beta4"
  },

  "commands": {
    "web-weblistener": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.WebListener --server.urls http://localhost:5000",
    "web-kestrel": "Microsoft.AspNet.Hosting --server Kestrel --server.urls http://localhost:5000"
  },

  "frameworks": {
    "dnx451": { },
    "dnxcore50": { }
  },

  "publishExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ],
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ]
}
